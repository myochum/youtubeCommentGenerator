{
    "name": "Monetate DE Utilization",
    "version": "0.0.3",
    "manifest_version": 2,
    "description": "Monitor your current weekly utilization through harvest.",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "default_locale": "en",
    "background": {
        "scripts": ["js/jquery-3.1.0.min.js", "src/bg/background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_icon": "icons/icon19.png",
        "default_title": "browser action demo",
        "default_popup": "src/browser_action/browser_action.html"
    },
    "permissions": [
        "tabs",
        "https://monetate.harvestapp.com/*"
    ],
    "content_scripts": [{
        "matches": [
            "https://monetate.harvestapp.com/*"
        ],
        "js": [
            "js/jquery-3.1.0.min.js",
            "src/inject/inject.js"
        ]
    }]
}